<!DOCTYPE html>
<html>
  <head>
    <title>RailsProjectReal</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <%= stylesheet_link_tag 'https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css' %>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>
  <body>

  <nav class="navbar navbar-expand-md navbar-dark bg-dark fixed-top">
  <a class="navbar-brand" href="#">TravelUSA </a>
  <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#root" aria-controls="navbarsExampleDefault" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>

  <div class="collapse navbar-collapse" id="navbarsExampleDefault">
    <ul class="navbar-nav mr-auto">

    <div class="topnav">
      <%= render '/layouts/header' %>
    </div>
      
    </ul>
    <ul class="nav navbar-nav navbar-right">
      <li><%= link_to "Sign Out", signout_path %></li>
    </ul>
  </div>
</nav>
<div class="container">
<%= yield %>
</div>



</body>
</html>
